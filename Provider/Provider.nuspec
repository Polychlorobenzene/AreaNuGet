      <dependency id="Newtonsoft.Json" version="11.0.1" />
      <dependency id="WebGrease" version="1.6.0" />
    </dependencies>    
    <contentFiles>
      <files include="Areas\Contact\Controllers\*.*" buildAction="Compile" copyToOutput="true" />
