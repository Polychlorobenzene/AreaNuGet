      <files include="Readme.md" buildAction="None" copyToOutput="true" />
    </contentFiles>
  </metadata>
  <files>
    <file src="Areas\Contact\Controllers\**" target="Content\Areas\Contact\Controllers\" />
    <file src="Areas\Contact\Views\**" target="Content\Areas\Contact\Views\" />
    <file src="Areas\Contact\Models\**" target="Content\Areas\Contact\Models\" />
    <file src="Areas\Contact\ContactAreaRegistration.cs" target="Content\Areas\Contact" />
    <file src="Readme.md" target="Content\Areas\Contact" />
  </files>
</package>