<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Contact</id>
    <version>0.1.0</version>
    <title>Contact Area</title>
    <authors>ESD</authors>
    <owners>ESD</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Contact Area for a consuming MVC project</description>
    <copyright>Copyright ©  2018</copyright>
    <dependencies>
      <dependency id="Antlr" version="3.5.0.2" />
      <dependency id="bootstrap" version="3.3.7" />
      <dependency id="jQuery" version="3.3.1" />
      <dependency id="jQuery.Validation" version="1.17.0" />
      <dependency id="Microsoft.ApplicationInsights.Web" version="2.5.1" />
      <dependency id="Microsoft.AspNet.Mvc" version="5.2.4" />
      <dependency id="Microsoft.AspNet.Web.Optimization" version="1.1.3" />
      <dependency id="Microsoft.CodeDom.Providers.DotNetCompilerPlatform" version="1.0.8" />
      <dependency id="Microsoft.jQuery.Unobtrusive.Validation" version="3.2.4" />
